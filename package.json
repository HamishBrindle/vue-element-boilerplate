{
  "name": "vue-cli-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "yarn element:generate -w & vue-cli-service serve",
    "build": "yarn element:generate && vue-cli-service build",
    "build:report": "yarn element:generate && vue-cli-service build --analyze",
    "lint": "vue-cli-service lint",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "test": "vue-cli-service test:unit",
    "element:variables": "node_modules/.bin/et -i src/assets/styles/vendor/_element-ui.scss",
    "element:generate": "(kill -9 $(lsof -i:8888 -t) 2> /dev/null || true) && node_modules/.bin/et",
    "gulp:styles": "node_modules/.bin/gulp jsToSass"
  },
  "element-theme": {
    "out": "./src/assets/styles/vendor/element-ui/css",
    "config": "./src/assets/styles/vendor/element-ui/scss/generated",
    "sassVariables": "./src/assets/styles/vendor/element-ui/scss/_element-ui.scss",
    "jsonVariables": "./src/assets/styles/json/_variables.json",
    "minimize": false
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.3",
    "core-js": "^2.6.5",
    "element-ui": "^2.4.5",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.0 || ^5.0.0",
    "@storybook/addon-knobs": "^4.1.0 || ^5.0.0",
    "@storybook/addon-links": "^4.1.0 || ^5.0.0",
    "@storybook/addon-notes": "^4.1.0 || ^5.0.0",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-wildcard": "^5.0.0",
    "before-build-webpack": "^0.2.8",
    "css-loader": "^2.1.1",
    "element-theme": "hamishbrindle/element-theme#master",
    "element-theme-chalk": "^2.7.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "node-sass-utils": "^1.1.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "style-resources-loader": "^1.2.1",
    "through2": "^3.0.1",
    "vue-cli-plugin-axios": "^0.0.4",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-cli-plugin-storybook": "^0.6.1",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.2.0",
    "vue-template-compiler": "^2.6.10"
  }
}
